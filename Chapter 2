# 2.63
'''
#include<cstdio>
#include<climits>
using namespace std;
unsigned srl(unsigned x,int k){
	unsigned xsra = (int)x>>k;
	int w = 8*sizeof(int);
	int msk = (-1)<<(w-k);
	return xsra & (~msk);
}
int sra(int x,int k){
	int xsrl = (unsigned)x>>k;
	int w = 8*sizeof(int);
	int msk = (-1)<<(w-k);
	int pos = (1<<(w-k-1)&xsrl);
	msk += (!pos)<<(w-k);
	return msk|xsrl;
}
int main(){
	int x = 346;
	printf("%x\n",sra(x,3));
	return 0;
}
'''
